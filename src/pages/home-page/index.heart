<h1 class="hero-title">WebGPU Explorer</h1>
<h2 class="hero-subtitle">Choose project to explore</h2>
<section class="panels-list">
  <project-panel
    x-for="project in projectList"
    class="panel"
    icon="{project.icon}"
    projectSlug="{project.projectSlug}"
    headline="{project.headline}"
    text="{project.text}"
  ></project-panel>
</section>